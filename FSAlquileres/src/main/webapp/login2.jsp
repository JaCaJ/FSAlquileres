	<body class="login-container">


		<!-- Login box start -->
		<div class="container">
                    <form action="#" id="form" name="form">
				<div class="login-box">
					<div class="login-blocks-img"></div>
					<div class="login-form">
						<a href="index.html" class="login-logo">
							<img src="assets/images/logo.svg" alt="Admin Dashboard Templates" />
						</a>
						<div class="login-welcome">
							Welcome back, <br />Please login to your account.
						</div>
						<div class="login-form-block">
							<label class="login-form-label">Username</label>
							<input type="text" class="login-form-control" />
						</div>
						<div class="login-form-block">
							<label class="login-form-label">Password</label>
                                                        <input type="password" class="login-form-control" id="pswrd" name="pswrd"/>
						</div>
						<div class="login-form-actions">
							<a href="forgot-password.html" class="btn-link">Forgot password?</a>
							<button type="submit" class="btn">
								<span class="icon"> <i class="icon-login"></i> </span> Login
							</button>
						</div>
						<div class="login-form-footer">
							<div class="additional-link">
								Don't have an account?
                                                                <a class="btn" id="boton" name="boton"> Signup</a>
							</div>
						</div>
					</div>
				</div>
			</form>
		</div>
		<!-- Login box end -->

		<!-- *************
			************ Required JavaScript Files *************
		************* -->
		<!-- Required jQuery first, then Bootstrap Bundle JS -->
		<script src="assets/js/jquery.min.js"></script>

		<!-- *************
			************ Vendor Js Files *************
		************* -->

		<!-- Main Js Required -->
		<script src="assets/js/main.js"></script>
